{
    "name": "godbout/chartjs-dash-docset",
    "description": "Chart.js Dash Docset, built with the Dash Docset Builder in PHP. 💹️📈️📉️📊️",
    "keywords": [
        "chart",
        "chartjs",
        "dash",
        "docset",
        "docsets",
        "offline",
        "documentation",
        "builder",
        "PHP",
        "love"
    ],
    "homepage": "https://github.com/godbout/chartjs-dash-docset",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/godbout/chartjs-dash-docset/issues",
        "source": "https://github.com/godbout/chartjs-dash-docset"
    },
    "authors": [
        {
            "name": "Guillaume Leclerc",
            "email": "guill@sleeplessmind.com.mo"
        }
    ],
    "require": {
        "godbout/dash-docset-builder": "^1.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.28",
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "test": "phpunit --color=always",
        "format": "php-cs-fixer fix -vvv --config=.php_cs.dist --ansi"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
